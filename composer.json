{
  "name": "yokai/pic-auth",
  "description": "pic-auth is a library to authenticate using images",
  "type": "library",
  "version": "0.0.0",
  "authors": [
    {
      "name": "ycatan-w",
      "homepage": "https://github.com/ycatan-w"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-gd": "*",
    "ext-imagick": "*",
    "jenssegers/imagehash": "^0.10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "phpunit/php-code-coverage": "^12.3",
    "friendsofphp/php-cs-fixer": "^3.87"
  },
  "autoload": {
    "psr-4": {
      "Crayon\\PicAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Crayon\\PicAuth\\": "tests/"
    }
  }
}
